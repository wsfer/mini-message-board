<!DOCTYPE html>
<html lang="en">
<%- include("partials/head", { title }) %>
<body>
    <%- include("partials/header") %>
    <main>
        <% messages.map(({ id, text, user, createdAt }) => { %>
            <p><%= user %></p>
            <p><%= text %></p>
            <p><%= createdAt %></p>
            <a href=<%= `/messages/message/${id}` %>>link</a>
        <% }) %>
    </main>
    <%- include("partials/footer") %>
</body>
</html>